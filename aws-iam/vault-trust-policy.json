{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VaultAssumeRole",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::YOUR_AWS_ACCOUNT_ID:root"
      },
      "Action": "sts:AssumeRole",
      "Condition": {
        "StringEquals": {
          "sts:ExternalId": "vault-aws-secrets-engine"
        }
      }
    }
  ]
}
